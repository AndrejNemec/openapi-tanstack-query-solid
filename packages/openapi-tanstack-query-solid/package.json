{
  "name": "openapi-tanstack-query-solid",
  "description": "A swagger client generator for typescript",
  "version": "1.0.2",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "bin": "dist/bin/otqs.js",
  "type": "commonjs",
  "main": "dist/index.js",
  "keywords": [
    "rest",
    "client",
    "swagger",
    "open-api",
    "fetch",
    "data fetching",
    "code-generation",
    "msw",
    "mock",
    "axios",
    "solidjs",
    "solid",
    "solid-query",
    "tanstack"
  ],
  "author": {
    "name": "Andrej Nemec"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AndrejNemec/openapi-tanstack-query-solid"
  },
  "scripts": {
    "build": "tsup ./src/bin/otqs.ts ./src/index.ts --target node12 --clean --dts",
    "dev": "tsup ./src/bin/otqs.ts ./src/index.ts --target node12 --clean --watch ./src --onSuccess 'yarn generate-api'",
    "lint": "eslint src/**/*.ts",
    "generate-api": "node ./dist/bin/otqs.js --config ../../samples/solid-query/basic/otqs.config.ts"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.6",
    "@types/js-yaml": "^4.0.8",
    "@types/lodash.uniq": "^4.5.8"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.1.0",
    "@openapi-tanstack-query-solid/axios": "1.0.2",
    "@openapi-tanstack-query-solid/core": "1.0.2",
    "@openapi-tanstack-query-solid/msw": "1.0.2",
    "@openapi-tanstack-query-solid/query": "1.0.2",
    "ajv": "^8.12.0",
    "cac": "^6.7.14",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "enquirer": "^2.4.1",
    "execa": "^5.1.1",
    "find-up": "5.0.0",
    "fs-extra": "^10.1.0",
    "lodash.uniq": "^4.5.0",
    "openapi-types": "^12.1.3",
    "openapi3-ts": "^3.2.0",
    "string-argv": "^0.3.2",
    "tsconfck": "^2.0.1"
  }
}
